#include "main.h"
#include <stdio.h>

/**
 * factorial - produce the factorial of a given number
 *
 * Return: 0;
 */

int factorial(int n)
{
if (n < 0)
{
return -1;
}
else if (n == 0)
{
return 1;
}
else
{
return n * factorial(n - 1);
}
}

int main(void)
{
int r;

r = factorial(5);
printf("%d\n", r);
}
